<tr id="<%= account_plan.id %>">
    <td><%= account_plan.descripcion %></td>
    <td><%= account_plan.estado %></td>
    <td><%= account_plan.version %></td>
    <td>
        <div class="row">
            <div class="col-xs-2">
                <%= link_to account_plan, remote: true, class: 'btn btn-primary', title: 'Ver', 'data-toggle' => 'tooltip', 'data-placement' => 'rigth' do %>
                    <span class= "glyphicon glyphicon-eye-open"></span>
                    <% end %>    
            </div>
            <div class="col-xs-2">
                <%= link_to edit_account_plan_path(account_plan), remote: true, class: 'btn btn-warning', title: 'Editar', 'data-toggle' => 'tooltip', 'data-placement' => 'rigth' do %>
                    <span class= "glyphicon glyphicon-pencil"></span>
                    <% end %>    
            </div>
            <div class="col-xs-2">
                <%= link_to account_plan, method: :delete, data: { confirm: 'Esta seguro?' }, class: 'btn btn-danger', title: 'Eliminar', 'data-toggle' => 'tooltip', 'data-placement' => 'rigth' do %>
                        <span class= "glyphicon glyphicon-trash"></span>
                    <% end %>                    
    
            </div>
            
        </div>
    </td>
</tr>