<div class="page-header">
  <%= link_to account_details_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_account_detail_path(@account_detail), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show account_detail</h1>
</div>

<dl class="dl-horizontal">
  <dt>Account:</dt>
  <dd><%= @account_detail.account_id %></dd>

  <dt>Service:</dt>
  <dd><%= @account_detail.service_id %></dd>

  <dt>Cantidad:</dt>
  <dd><%= @account_detail.cantidad %></dd>

  <dt>Precio:</dt>
  <dd><%= @account_detail.precio %></dd>

  <dt>Subtotal:</dt>
  <dd><%= @account_detail.subtotal %></dd>

</dl>
