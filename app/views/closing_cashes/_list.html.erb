
<div id="filterrific_results">



<div class="box box-primary">
 <div class="box-header with-border no-print"  style="text-align:center">
  <h1 class="box-title">Listado de ingresos en caja</h1>
   <div class="input-group-control pull-right">
   
       <%= link_to "Imprimir", '#', class: "btn btn-primary no-print", 
            onclick: 'window.print();return false;' %>
    

    </div>
  </div>

<div class="box-body no-print">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:30%">
    <col style="width:30%">
    <col style="width:40%">
  </colgroup>
    <thead>
      <tr>
        <th>Caja</th>
        <th>Fecha de cierre de la caja</th>
        <th>Ingreso en la caja</th>
        
      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @closing_cashes.order(id: :desc)) do |closing_cash| %>
       <td><%= closing_cash.opening_cash.cash.descripcion %></td>
       <td><%= closing_cash.fecha_cierre %></td>
       <td><%= closing_cash.monto_efectivo.to_i+closing_cash.monto_cheque.to_i+closing_cash.monto_tcredito.to_i+closing_cash.monto_tdebito.to_i %></td>
       
    <% end %>
    
    </tbody>
     
  </table>
  <div class="box-footer clearfix" style="margin-right:-10px">
    <ul class="pagination pagination-sm no-margin pull-right">
    <%= will_paginate @closing_cashes, renderer: BootstrapPagination::Rails, :container => false %>
    </ul>
    </div>
  </div>
 
 
 

</div>

<div class="box-body hide">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:20%">
    <col style="width:20%">
    <col style="width:40%">
    <col style="width:20%">
  </colgroup>
    <thead>
      <tr>
        <th>Caja</th>
        <th>Fecha de cierre de la caja</th>
        <th>Ingreso en la caja</th>
        
      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @closing_cashes.order(id: :desc)) do |closing_cash| %>
       <td><%= closing_cash.opening_cash.cash.descripcion %></td>
       <td><%= closing_cash.fecha_cierre %></td>
       <td><%= closing_cash.monto_efectivo.to_i+closing_cash.monto_cheque.to_i+closing_cash.monto_tcredito.to_i+closing_cash.monto_tdebito.to_i %></td>
       
    <% end %>
    
    </tbody>

  </table>
  </div>
 

 
   

</div>
</div>
</div>