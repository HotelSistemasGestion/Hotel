<tr id="<%= accounting_entry.id %>">
    <td><%= accounting_entry.numero %></td>
    
    <% accounting_entry.account_x_entries.each do |details|%>
                        <td><%= details.observacion%></td>
     <%end%>                    
    <td><%= accounting_entry.fecha %></td>
    <td>
        <div class="row">
            <div class="col-xs-2">
                <%= link_to accounting_entry, remote: true, class: 'btn btn-primary', title: 'Ver', 'data-toggle' => 'tooltip', 'data-placement' => 'rigth' do %>
                    <span class= "glyphicon glyphicon-eye-open"></span>
                    <% end %>    
            </div>
        </div>
    </td>
</tr>
    