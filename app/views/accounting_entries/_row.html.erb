<tr id="<%= accounting_entry.id %>">
    <td><%= accounting_entry.numero %></td>
    <td><%= accounting_entry.fecha %></td>
    <td>
        <div class="row">
            <div class="col-xs-2">
                <%= link_to accounting_entry, remote: true, class: 'btn btn-primary', title: 'Ver', 'data-toggle' => 'tooltip', 'data-placement' => 'rigth' do %>
                    <span class= "glyphicon glyphicon-eye-open"></span>
                    <% end %>    
            </div>
            <div class="col-xs-2">
                <%= link_to accounting_entry, method: :delete, data: { confirm: 'Esta seguro?' }, class: 'btn btn-danger', title: 'Eliminar', 'data-toggle' => 'tooltip', 'data-placement' => 'rigth' do %>
                        <span class= "glyphicon glyphicon-trash"></span>
                    <% end %>                    
    
            </div>
            
        </div>
    </td>
</tr>