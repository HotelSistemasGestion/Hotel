<div class="page-header">
  <%= link_to reservation_requests_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Atras
  <% end %>
  <%= link_to edit_budget_path(@budget), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show budget</h1>
</div>

<dl class="dl-horizontal">
  <dt>Reservation request:</dt>
  <dd><%= @budget.reservation_request_id %></dd>

  <dt>Email:</dt>
  <dd><%= @budget.email %></dd>

  <dt>Dias:</dt>
  <dd><%= @budget.dias %></dd>

  <dt>Descuento:</dt>
  <dd><%= @budget.descuento %> %</dd>

  <dt>Total:</dt>
  <dd><%= @budget.total %></dd>

</dl>
