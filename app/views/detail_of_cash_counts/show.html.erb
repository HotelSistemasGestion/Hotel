<div class="page-header">
  <%= link_to detail_of_cash_counts_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_detail_of_cash_count_path(@detail_of_cash_count), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show detail_of_cash_count</h1>
</div>

<dl class="dl-horizontal">

  <dt>Monto sistema:</dt>
  <dd><%= @detail_of_cash_count.monto_sistema %></dd>


  <dt>Monto caja:</dt>
  <dd><%= @detail_of_cash_count.monto_caja %></dd>


</dl>
