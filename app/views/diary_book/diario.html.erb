<div class="box"> 
  <div class="row">
       <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="box-header with-border">
                                   <h3 class="box-title titulo">Libro Diario</h3>
                      </div>
                                          
                          <div class="box-body">
                                <form class="form-inline">
   
                                <div class="col-xs-6 col-md-6 col-sm-6">
                                  <%= form_tag diary_book_diario_path ,remote: true, method: :post , id: "_search_conta" do %>
                                     <%=label_tag "Periodo: "%>
                                     <%= text_field_tag :search_conta, params[:search_conta],class: "form-control"%>
                                     <%= button_tag(type: :submit, class: "btn btn-primary no-print") do %>
                                              <i class="glyphicon glyphicon-search"></i>
                                          <% end %>
                                      <%end%>
                                             
                                      <select class="selectpicker no-print" id = "my-select"data-style="btn-primary" data-width="fit">
                                        <%ejer_all=AccountingYear.all.select('anho')%>
                                        <%con_ejer=0%>
                                        <%while ejer_all.count>con_ejer%>
                                          <option><%=ejer_all[con_ejer].anho%> </option>
                                          <%con_ejer+=1%>
                                        <% end%>
                                        </select>         
                                               
                                      </div>
                                      <div class="col-lg-6 col-md-6 col-sm-6">
                                      <h4 style="text-align: center">Denominacion o razon social: Hotel Uni</h4>
                                      <%= link_to ''.html_safe, '#', class: "btn btn-primary no-print glyphicon glyphicon-print alineado", onclick: 'window.print();return false;'%>
                                    </div>
                                </form>
                                    <%= render :partial=>'venta' %>

                </div>
          </div>
       </div>
</div>
        