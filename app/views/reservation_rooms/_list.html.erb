
<div id="filterrific_results">



<div class="box box-primary">
 <div class="box-header with-border no-print"  style="text-align:center">
  <h1 class="box-title">Listado de pedido de reservaciones</h1>
   <div class="input-group-control pull-right">
   
       <%= link_to "Imprimir", '#', class: "btn btn-primary no-print", 
            onclick: 'window.print();return false;' %>
    

    </div>
  </div>

<div class="box-body no-print">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:15%">
    <col style="width:15%">
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:10%">
  </colgroup>
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Email</th>
        <th>Habitacion</th>
        <th>Tipo de Habitacion</th>
        <th>Tipo de Comfort</th>
        <th>CheckIn</th>
        <th>CheckOut</th>
        <th>Estado</th>
      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @reservation_rooms.order(id: :desc)) do |reservation_rooms| %>
       <td><%= reservation_rooms.reservation.nombre %></td>
       <td><%= reservation_rooms.reservation.apellido %></td>
       <td><%= reservation_rooms.reservation.email %></td>
       <td><%= reservation_rooms.room.identificador %></td>
       <td><%= reservation_rooms.type_of_room.tipo %></td>
       <td><%= reservation_rooms.comfort.descripcion %></td>
      <td><%= reservation_rooms.check_in %></td>
      <td><%= reservation_rooms.check_out %></td>
      <td><%= reservation_rooms.reservation.state %></td>
    <% end %>
    
    </tbody>
     
  </table>
  <div class="box-footer clearfix" style="margin-right:-10px">
    <ul class="pagination pagination-sm no-margin pull-right">
    <%= will_paginate @reservation_rooms, renderer: BootstrapPagination::Rails, :container => false %>
    </ul>
    </div>
  </div>
 
 
 

</div>

<div class="box-body hide">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:15%">
    <col style="width:15%">
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:10%">
  </colgroup>
    thead>
      <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Email</th>
        <th>Habitacion</th>
        <th>Tipo de Habitacion</th>
        <th>Tipo de Comfort</th>
        <th>CheckIn</th>
        <th>CheckOut</th>
        <th>Estado</th>

      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @reservation_rooms_report.order(id: :desc)) do |reservation_rooms| %>
       <td><%= reservation_rooms.reservation.nombre %></td>
       <td><%= reservation_rooms.reservation.apellido %></td>
       <td><%= reservation_rooms.reservation.email %></td>
       <td><%= reservation_rooms.room.identificador %></td>
       <td><%= reservation_rooms.type_of_room.tipo %></td>
       <td><%= reservation_rooms.comfort.descripcion %></td>
      <td><%= reservation_rooms.check_in %></td>
      <td><%= reservation_rooms.check_out %></td>
      <td><%= reservation_rooms.reservation.state %></td>
    <% end %>
    
    </tbody>
     

  </table>
  </div>
 

 
   

</div>
</div>
</div>