<div class="page-header">
  <%= link_to reservation_rooms_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_reservation_room_path(@reservation_room), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show reservation_room</h1>
</div>

<dl class="dl-horizontal">

  <dt>Reservation:</dt>
  <dd><%= @reservation_room.reservation_id %></dd>


  <dt>Room:</dt>
  <dd><%= @reservation_room.room_id %></dd>


  <dt>Subtotal:</dt>
  <dd><%= @reservation_room.subtotal %></dd>


</dl>
