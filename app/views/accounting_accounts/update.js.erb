<% if @accounting_account.errors.blank? %>
  // add the row to the table, show alert and hide modal
  $('#product-modal').modal('hide');
  $('table tbody tr#<%= @accounting_account.id %>').replaceWith(' <%= j render "row", accounting_account: @accounting_account %>');
  alert('La cuenta se modifico exitosamente');
<% else %>
  alert('<%= @accounting_account.errors.full_messages[0] %>');
<% end %>