<div class="page-header">
  <%= link_to accounting_accounts_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_accounting_account_path(@accounting_account), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show accounting_account</h1>
</div>

<dl class="dl-horizontal">
  <dt>Cuenta:</dt>
  <dd><%= @accounting_account.cuenta %></dd>

  <dt>Grupo:</dt>
  <dd><%= @accounting_account.grupo %></dd>


  <dt>Nombre:</dt>
  <dd><%= @accounting_account.nombre %></dd>


  <dt>Imputable:</dt>
  <dd><%= @accounting_account.imputable %></dd>


</dl>
