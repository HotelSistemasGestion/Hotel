<div class="page-header">
  <%= link_to new_accounting_account_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Accounting account
  <% end %>
  <h1>Listing accounting_accounts</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>Cuenta</th>
        <th>Grupo</th>
    
        <th>Nombre</th>
    
        <th>Imputable</th>
    
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @accounting_accounts) do |accounting_account| %>
      <td><%= accounting_account.cuenta%></td>
    
        <td><%= accounting_account.grupo %></td>
    
        <td><%= accounting_account.nombre %></td>
    
        <td><%= accounting_account.imputable %></td>
    
        <td><%= link_to 'Show', accounting_account %></td>
        <td><%= link_to 'Edit', edit_accounting_account_path(accounting_account) %></td>
        <td><%= link_to 'Destroy', accounting_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
