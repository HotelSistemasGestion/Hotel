<div class="page-header">
  <%= link_to new_type_of_room_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Type of room
  <% end %>
  <h1>Listing type_of_rooms</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Tipo</th>
    
        <th>Descripcion</th>
    
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @type_of_rooms) do |type_of_room| %>
    
        <td><%= type_of_room.tipo %></td>
    
        <td><%= type_of_room.descripcion %></td>
    
        <td><%= link_to 'Show', type_of_room %></td>
        <td><%= link_to 'Edit', edit_type_of_room_path(type_of_room) %></td>
        <td><%= link_to 'Destroy', type_of_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
