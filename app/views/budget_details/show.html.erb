<div class="page-header">
  <%= link_to budget_details_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_budget_detail_path(@budget_detail), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show budget_detail</h1>
</div>

<dl class="dl-horizontal">
  <dt>Budget:</dt>
  <dd><%= @budget_detail.budget_id %></dd>

  <dt>Cantidad:</dt>
  <dd><%= @budget_detail.cantidad %></dd>

  <dt>Service:</dt>
  <dd><%= @budget_detail.service_id %></dd>

  <dt>Subtotal:</dt>
  <dd><%= @budget_detail.subtotal %></dd>

</dl>
