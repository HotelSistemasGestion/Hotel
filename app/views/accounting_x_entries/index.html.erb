<div class="page-header">
  <%= link_to new_accounting_x_entry_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Accounting x entry
  <% end %>
  <h1>Listing accounting_x_entries</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Monto</th>
    
        <th>Observacion</th>
    
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @accounting_x_entries) do |accounting_x_entry| %>
    
        <td><%= accounting_x_entry.monto %></td>
    
        <td><%= accounting_x_entry.observacion %></td>
    
        <td><%= link_to 'Show', accounting_x_entry %></td>
        <td><%= link_to 'Edit', edit_accounting_x_entry_path(accounting_x_entry) %></td>
        <td><%= link_to 'Destroy', accounting_x_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
