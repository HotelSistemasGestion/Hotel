
<div id="filterrific_results">



<div class="box box-primary">
 <div class="box-header with-border no-print"  style="text-align:center">
  <h1 class="box-title">Listado de quejas</h1>
   <div class="input-group-control pull-right">
   
       <%= link_to "Imprimir", '#', class: "btn btn-primary no-print", 
            onclick: 'window.print();return false;' %>
    

    </div>
  </div>

<div class="box-body">
<div class="table-responsive">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:20%">
    <col style="width:20%">
    <col style="width:40%">
    <col style="width:20%">
  </colgroup>
    <thead>
      <tr>
        <th>Habitacion</th>
        <th>Servicio relacionado</th>
        <th>Queja</th>
        <th>Fecha</th>
      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @complaints.order(id: :desc)) do |complaint| %>
       <td><%= complaint.room.identificador %></td>
       <td><%= complaint.complaint_service.description + " - " + complaint.service_description.to_s%></td>
       <td><%= complaint.description %></td>
       <td><%= complaint.created_at.strftime("%F %T") %></td>
    <% end %>
    </tbody>

  </table>
  </div>
 

 
   
</div>
</div>
</div>
