<div class="page-header">
  <%= link_to new_accounting_year_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Accounting year
  <% end %>
  <h1>Listing accounting_years</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Anho</th>
    
        <th>Estado</th>
    
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @accounting_years) do |accounting_year| %>
    
        <td><%= accounting_year.anho %></td>
    
        <td><%= accounting_year.estado %></td>
    
        <td><%= link_to 'Show', accounting_year %></td>
        <td><%= link_to 'Edit', edit_accounting_year_path(accounting_year) %></td>
        <td><%= link_to 'Destroy', accounting_year, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
