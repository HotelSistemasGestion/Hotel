<div class="page-header">
  <%= link_to budget_room_details_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_budget_room_detail_path(@budget_room_detail), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show budget_room_detail</h1>
</div>

<dl class="dl-horizontal">
  <dt>Budget:</dt>
  <dd><%= @budget_room_detail.budget %></dd>

  <dt>Cantidad:</dt>
  <dd><%= @budget_room_detail.cantidad %></dd>

  <dt>Type of room:</dt>
  <dd><%= @budget_room_detail.type_of_room %></dd>

  <dt>Subtotal:</dt>
  <dd><%= @budget_room_detail.subtotal %></dd>

</dl>
