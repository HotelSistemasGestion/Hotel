
<div id="filterrific_results">



<div class="box box-primary">
 <div class="box-header with-border no-print"  style="text-align:center">
  <h1 class="box-title">Listado de Servicios</h1>
   <div class="input-group-control pull-right">
   
       <%= link_to "Imprimir", '#', class: "btn btn-primary no-print", 
            onclick: 'window.print();return false;' %>
    

    </div>
  </div>

<div class="box-body no-print">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:30%">
    <col style="width:50%">
    <col style="width:20%">
  
  </colgroup>
    <thead>
      <tr>
        <th>Servicio</th>
        <th>Descripcion del Servicio</th>
        <th>Precio</th>
      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @services.order(id: :desc)) do |service| %>
       <td><%= service.nombre %></td>
       <td><%= service.descripcion %></td>
       <td><%= service.precio %></td>
    <% end %>
    
    </tbody>
     
  </table>
  <div class="box-footer clearfix" style="margin-right:-10px">
    <ul class="pagination pagination-sm no-margin pull-right">
    <%= will_paginate @services, renderer: BootstrapPagination::Rails, :container => false %>
    </ul>
    </div>
  </div>
 
 
 

</div>

<div class="box-body hide">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:30%">
    <col style="width:50%">
    <col style="width:20%">
  </colgroup>
    <thead>
      <tr>
        <th>Servicio</th>
        <th>Descripcion del Servicio</th>
        <th>Precio</th>
      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @services_report.order(id: :desc)) do |service| %>
       <td><%= service.nombre %></td>
       <td><%= service.descripcion %></td>
       <td><%= service.precio %></td>
    <% end %>
    
    </tbody>

  </table>
  </div>
 

 
   

</div>
</div>
</div>