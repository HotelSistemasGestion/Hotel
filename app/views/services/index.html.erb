<div class="box">
      <div class="box-header with-border">
      <form class="form-inline">
         <!--Div selector de rango de fechas-->
        <div class="col-xs-6 col-md-6 col-sm-6">
              <h3 class="box-title titulo">Servicios</h3>

              
            </div>
            <div class="col-xs-6 col-md-6 col-sm-6 text-right">
              <%= link_to 'NuevoServicio', new_service_path, class: "btn btn-primary glyphicon glyphicon-plus" %> 
               
          </div>
        </form>
      </div>


       <div class="box-body">
          <div class="dataTables_wrapper form-inline dt-bootstrap">
            <div class="row">
              <div class="col-sm-6"></div>
              <div class="col-sm-6"></div>
            </div>
            
            <div class="row">
              <div class="col-sm-offset-1 col-sm-9">
                <table class="table table-bordered  table-responsive ">
                  <thead>
                    <tr role="row">
            <th class="">Nombre</th>
            <th class="">Descripcion</th>
            <th class="">Precio</th>
            <th class="col-sm-3"></th>
          </tr>
        </thead>
        <tbody>
          <% @services.each do |service| %>
            <%= render "row", service: service %>
          <% end %>
        </tbody>
      </table>
    </div>

</div>
</div>
</div>
</div>
</div>