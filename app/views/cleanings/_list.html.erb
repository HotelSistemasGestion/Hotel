
<div id="filterrific_results">



<div class="box box-primary">
 <div class="box-header with-border no-print"  style="text-align:center">
  <h1 class="box-title">Listado de Limpiezas</h1>
   <div class="input-group-control pull-right">
   
       <%= link_to "Imprimir", '#', class: "btn btn-primary no-print", 
            onclick: 'window.print();return false;' %>
    

    </div>
  </div>

<div class="box-body no-print">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:20%">
    <col style="width:20%">
    <col style="width:40%">
    <col style="width:20%">
  </colgroup>
    <thead>
      <tr>
        <th>Habitacion</th>
        <th>Funcionario encargado</th>
        <th>Fecha de inicio</th>
        <th>Fecha de fin</th>
      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @cleanings.order(id: :desc)) do |cleaning| %>
       <td><%= cleaning.room.identificador %></td>
       <td><%= cleaning.employee.nombre %></td>
       <td><%= cleaning.start %></td>
       <td><%= cleaning.end %></td>
    <% end %>
    
    </tbody>
     
  </table>
  <div class="box-footer clearfix" style="margin-right:-10px">
    <ul class="pagination pagination-sm no-margin pull-right">
    <%= will_paginate @cleanings, renderer: BootstrapPagination::Rails, :container => false %>
    </ul>
    </div>
  </div>
 
 
 

</div>

<div class="box-body hide">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
