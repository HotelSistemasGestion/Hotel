<div class="box-body">
  <%= link_to cleaning_rooms_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_cleaning_path(@cleaning), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Limpieza de Habitacion</h1>


<dl class="dl-horizontal">


  <dt>Habitacion:</dt>
  <dd><%= @cleaning.room_identificador %></dd>

  <dt>Desde:</dt>
  <dd><%= @cleaning.start %></dd>

  <dt>Hasta:</dt>
  <dd><%= @cleaning.end %></dd>

  <dt>Titulo:</dt>
  <dd><%= @cleaning.title %></dd>

</dl>
</div>