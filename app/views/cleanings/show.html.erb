<div class="page-header">
  <%= link_to cleanings_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_cleaning_path(@cleaning), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show cleaning</h1>
</div>

<dl class="dl-horizontal">
  <dt>Cleaning room:</dt>
  <dd><%= @cleaning.cleaning_room %></dd>

  <dt>Room:</dt>
  <dd><%= @cleaning.room %></dd>

  <dt>Start:</dt>
  <dd><%= @cleaning.start %></dd>

  <dt>End:</dt>
  <dd><%= @cleaning.end %></dd>

  <dt>Title:</dt>
  <dd><%= @cleaning.title %></dd>

</dl>
