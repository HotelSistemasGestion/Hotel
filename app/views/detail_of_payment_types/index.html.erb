<div class="page-header">
  <%= link_to new_detail_of_payment_type_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Detail of payment type
  <% end %>
  <h1>Listing detail_of_payment_types</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Titular</th>
    
        <th>Banco</th>
    
        <th>Numero</th>
    
        <th>Tipo tarjeta</th>
    
        <th>Fecha disponibilidad</th>
    
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @detail_of_payment_types) do |detail_of_payment_type| %>
    
        <td><%= detail_of_payment_type.titular %></td>
    
        <td><%= detail_of_payment_type.banco %></td>
    
        <td><%= detail_of_payment_type.numero %></td>
    
        <td><%= detail_of_payment_type.tipo_tarjeta %></td>
    
        <td><%= detail_of_payment_type.fecha_disponibilidad %></td>
    
        <td><%= link_to 'Show', detail_of_payment_type %></td>
        <td><%= link_to 'Edit', edit_detail_of_payment_type_path(detail_of_payment_type) %></td>
        <td><%= link_to 'Destroy', detail_of_payment_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
