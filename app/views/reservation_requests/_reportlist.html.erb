
<div id="filterrific_results">



<div class="box box-primary">
 <div class="box-header with-border no-print"  style="text-align:center">
  <h1 class="box-title">Listado de pedido de reservaciones</h1>
   <div class="input-group-control pull-right">
   
       <%= link_to "Imprimir", '#', class: "btn btn-primary no-print", 
            onclick: 'window.print();return false;' %>
    

    </div>
  </div>

<div class="box-body no-print">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:10%">
    <col style="width:15%">
    <col style="width:15%">
    <col style="width:15%">
    <col style="width:15%">
  </colgroup>
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Email</th>
        <th>Cantidad de niños</th>
        <th>Cantidad de adultos</th>
        <th>Cantidad de familias</th>
        <th>CheckIn</th>
        <th>CheckOut</th>
      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @reservation_requests.order(id: :desc)) do |reservation_request| %>
       <td><%= reservation_request.nombre %></td>
       <td><%= reservation_request.apellido %></td>
       <td><%= reservation_request.email %></td>
       <td><%= reservation_request.cantidad_de_ninhos %></td>
      <td><%= reservation_request.cantidad_de_adultos %></td>
      <td><%= reservation_request.cantidad_de_familias %></td>
      <td><%= reservation_request.check_in %></td>
      <td><%= reservation_request.check_out %></td>
    <% end %>
    
    </tbody>
     
  </table>
  <div class="box-footer clearfix" style="margin-right:-10px">
    <ul class="pagination pagination-sm no-margin pull-right">
    <%= will_paginate @reservation_requests, renderer: BootstrapPagination::Rails, :container => false %>
    </ul>
    </div>
  </div>
 
 
 

</div>

<div class="box-body hide">
   <div id="filterrific_results">
  <table class="table table-striped table-bordered table-hover">
  <colgroup>
    <col style="width:20%">
    <col style="width:20%">
    <col style="width:40%">
    <col style="width:20%">
  </colgroup>
    <thead>
      <tr>
         <th>Nombre</th>
        <th>Apellido</th>
        <th>Email</th>
        <th>Cantidad de niños</th>
        <th>Cantidad de adultos</th>
        <th>Cantidad de familias</th>
        <th>CheckIn</th>
        <th>CheckOut</th>
      </tr>
    </thead>

   
    <tbody>
    
    
      <%= content_tag_for(:tr, @reservation_requests.order(id: :desc)) do |reservation_request| %>
       <td><%= reservation_request.nombre %></td>
       <td><%= reservation_request.apellido %></td>
       <td><%= reservation_request.email %></td>
       <td><%= reservation_request.cantidad_de_ninhos %></td>
      <td><%= reservation_request.cantidad_de_adultos %></td>
      <td><%= reservation_request.cantidad_de_familias %></td>
      <td><%= reservation_request.check_in %></td>
      <td><%= reservation_request.check_out %></td>
    <% end %>
    </tbody>

  </table>
  </div>
 

 
   

</div>
</div>
</div>