<div class="page-header">
  <%= link_to cleaning_rooms_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_cleaning_room_path(@cleaning_room), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show cleaning_room</h1>
</div>

<dl class="dl-horizontal">
  <dt>Room:</dt>
  <dd><%= @cleaning_room.room %></dd>

  <dt>Employee:</dt>
  <dd><%= @cleaning_room.employee %></dd>

  <dt>Day:</dt>
  <dd><%= @cleaning_room.day %></dd>

</dl>
